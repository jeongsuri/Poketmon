<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.choongang.mypage.mapper.MyPageMapper">

    <resultMap id="myPageMap" type="org.choongang.mypage.entities.Mypage">
        <result column="USER_NO" property="userNo" />
        <result column="POKEMON_NO" property="pokemonNo" />
        <result column="NICKNAME" property="nickName" />
    </resultMap>

   <!--   로그인된 내 아이디로 맴버 테이블에서 유저 넘버를 찾음
    <select id="mypokeList" resultMap="myPageMap">
        SELECT * FROM MEMBER WHERE USER_NO = #{userNo}
    </select> -->


